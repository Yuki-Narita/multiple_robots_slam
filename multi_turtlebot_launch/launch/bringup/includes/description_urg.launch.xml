<!--
  Robot description only (used in private and paired master).
 -->
<launch>
  <arg name="base"/>
  <arg name="stacks"/>
  <arg name="3d_sensor"/>
  
  <arg name="robot_name"/>

  <arg name="urdf_file" default="$(find xacro)/xacro.py '$(find multi_turtlebot_gazebo)/world/models/robot/labs_turtlebot2_urg.urdf.xacro'" />

  <param name="robot_description" command="$(arg urdf_file)" />

</launch>
